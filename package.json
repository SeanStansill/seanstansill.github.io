{
	"name": "seanstansill.github.io",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@smui-extra/accordion": "^7.0.0-beta.0",
		"@smui/button": "^7.0.0-beta.0",
		"@smui/common": "^7.0.0-beta.0",
		"@smui/drawer": "^7.0.0-beta.0",
		"@smui/icon-button": "^7.0.0-beta.0",
		"@smui/list": "^7.0.0-beta.0",
		"@smui/menu": "^7.0.0-beta.0",
		"@smui/paper": "^7.0.0-beta.0",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-static": "^1.0.4",
		"@sveltejs/kit": "^1.0.0",
		"@tsconfig/svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"smui-theme": "^7.0.0-beta.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.2",
		"svelte-preprocess": "^5.0.0",
		"typescript": "^4.9.4",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-static": "^1.0.4",
		"katex": "^0.16.4",
		"svelte-katex": "^0.1.2"
	}
}
